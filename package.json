{
  "name": "formulon",
  "version": "0.0.0-development",
  "description": "Salesforce Formula Parser",
  "main": "lib/formulon.js",
  "scripts": {
    "build": "yarn compile:parser && babel src --out-dir lib",
    "compile:parser": "pegjs -o ./lib/salesforceParser.js ./grammars/salesforceFormula.pegjs",
    "test": "yarn build && mocha --require @babel/register --recursive",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/leifg/formulon.git"
  },
  "keywords": [
    "parser",
    "salesforce",
    "formula"
  ],
  "author": "Leif Gensert",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@babel/register": "^7.8.3",
    "chai": "^4.2.0",
    "mocha": "^7.0.1",
    "pegjs": "^0.10.0",
    "semantic-release": "^17.0.4"
  }
}
